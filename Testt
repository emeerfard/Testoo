<script type='text/javascript'>
$(function() {
    $(&#39;.menuku li&#39;).hover(function() {
        $(this).children(&#39;ul&#39;).filter(&#39;:not(:animated)&#39;).show(200);
    }, function() {
        $(this).children(&#39;ul&#39;).hide(400);
    });
});
